        },
        "released": { 
          "action" : "set", "path": "control/disable", "args": [ true ]
        }
      }
    },
    "PushButton_controlAutomatic": {
      "signals": {
        "pressed": { 
          "action" : "set", "path": "control/automatic", "args": [
            { "type": "float", "ref": "control/automatic/speed", "default": 0.5 }
           ]
        }
      }
    },
    "PushButton_controlDisable": {
      "signals": {
        "pressed": { 
          "action" : "set", "path": "control/disable", "args": [ true ]
        }
      }
    }    
  },
  "QLineEdit": {
