{
  "QPushButton": {
    "PushButton_controlManualFW": {
      "signals": {
        "pressed": { 
          "action" : "set", 
          "path": "control/manual", 
          "args": [
            "FW",
            { "type": "float", "ref": "control/manual/speed", "default": 0.5 },
            { "type": "float", "ref": "control/manual/duration", "default": 0.5 }
          ]
        },
        "released": { 
          "action" : "set", "path": "control/disable", "args": [ true ]
        }
      }
    },
    "PushButton_controlManualBW": {
      "signals": {
        "pressed": { 
          "action" : "set", 
          "path": "control/manual", 
          "args": [
