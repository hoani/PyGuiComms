  "QVBoxLayout": {
    "VBox_Imu":{
      "insert": [
        {
          "widget": "plotVec3",
          "settings": { "title": "Accelerometers (m.s-2)" },
          "subscriptions": {
            "imu/accel": { "callback": "setVec3", "config": [] }
          } 
        },
        {
          "widget": "plotVec3",
          "settings": { "title": "Gyroscopes (deg.s-1)" },
          "subscriptions": { 
            "imu/gyros": { "callback": "setVec3", "config": [] }
          } 
        },
        {
          "widget": "plotVec3",
          "settings": { "title": "Magnetometers (mT)" },
          "subscriptions": {
            "imu/magne": { "callback": "setVec3", "config": [] } 
          } 
        }
      ]
    }   
  }
